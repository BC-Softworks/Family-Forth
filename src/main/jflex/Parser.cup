nonterminal com.famiforth.Absyn.Prg Prg;
nonterminal com.famiforth.Absyn.ListStatement ListStatement;
nonterminal com.famiforth.Absyn.Statement Statement;
nonterminal com.famiforth.Absyn.Expr Expr;
nonterminal com.famiforth.Absyn.Word Word;
nonterminal com.famiforth.Absyn.Constant Constant;
nonterminal com.famiforth.Absyn.ListExpr ListExpr;

terminal _SYMB_0;    //   :
terminal _SYMB_1;    //   ;
terminal _SYMB_2;    //   begin
terminal _SYMB_3;    //   if
terminal _SYMB_4;    //   then
terminal _SYMB_5;    //   until



terminal Integer _INTEGER_;



terminal String ForthWord;


start with Prg;


Prg ::= ListStatement:p_1  {: RESULT = new com.famiforth.Absyn.Program(p_1); :}
;
ListStatement ::= Statement:p_1  {: RESULT = new com.famiforth.Absyn.ListStatement(); RESULT.addLast(p_1); :}
  | Statement:p_1 ListStatement:p_2  {: RESULT = p_2; p_2.addFirst(p_1); :}
;
Statement ::= Constant:p_1  {: RESULT = new com.famiforth.Absyn.Literal(p_1); :}
  | Expr:p_1  {: RESULT = new com.famiforth.Absyn.Expression(p_1); :}
  | Word:p_1  {: RESULT = new com.famiforth.Absyn.SinlgeWord(p_1); :}
;
Expr ::= _SYMB_3:p_1 Statement:p_2 _SYMB_4:p_3  {: RESULT = new com.famiforth.Absyn.Conditional(p_2); :}
  | _SYMB_2:p_1 Statement:p_2 _SYMB_5:p_3  {: RESULT = new com.famiforth.Absyn.Loop(p_2); :}
  | _SYMB_0:p_1 Word:p_2 Statement:p_3 _SYMB_1:p_4  {: RESULT = new com.famiforth.Absyn.Define(p_2,p_3); :}
;
Word ::= ForthWord:p_1  {: RESULT = new com.famiforth.Absyn.Name(p_1); :}
;
Constant ::= _INTEGER_:p_1  {: RESULT = new com.famiforth.Absyn.Number(p_1); :}
;
ListExpr ::= Expr:p_1  {: RESULT = new com.famiforth.Absyn.ListExpr(); RESULT.addLast(p_1); :}
  | Expr:p_1 ListExpr:p_2  {: RESULT = p_2; p_2.addFirst(p_1); :}
;

