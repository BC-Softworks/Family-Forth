{
    "$schema": "../../../../schema/testcase.schema.json",
    "target": {
        "testType": "JSR",
        "start": {
            "label": "zeroequal"
        }
    },
    "testCases": {
        "ZERO EQUALS ZERO": {
            "setup": {
                "register": {
                    "X": {
                        "value": "$FE"
                    }
                },
                "memory": [
                    {
                        "address": "$FE",
                        "value": 0
                    },
                    {
                        "address": "$FF",
                        "value": 0
                    }
                ]
                
            },
            "expected": {
                "register": {
                    "X": {
                        "value": { 
                            "eq": "$FE"
                        }
                    }
                },
                "memory": [
                    {
                        "address": "$FE",
                        "value": {
                            "eq": "$FF"
                        }
                    },
                    {
                        "address": "$FF",
                        "value": {
                            "eq": "$FF"
                        }
                    }
                ]
            }
        },
        "ZERO EQUALS ONE": {
            "setup": {
                "register": {
                    "X": {
                        "value": "$FE"
                    }
                },
                "memory": [
                    {
                        "address": "$FE",
                        "value": "$FF"
                    },
                    {
                        "address": "$FF",
                        "value": "$FF"
                    }
                ]
                
            },
            "expected": {
                "register": {
                    "X": {
                        "value": { 
                            "eq": "$FE"
                        }
                    }
                },
                "memory": [
                    {
                        "address": "$FE",
                        "value": {
                            "eq": 0
                        }
                    },
                    {
                        "address": "$FF",
                        "value": {
                            "eq": 0
                        }
                    }
                ]
            }
        }
    },
    "config": {
        "testMemoryAccess": {
            "feature": [
                "ACCESS_TO_OUT_OF_SEGMENT"
            ],
            "ignoreList": [
                {
                    "start": {
                        "address": "$0"
                    },
                    "size": "$800"
                },
                {
                    "start": {
                        "address": "$2000"
                    },
                    "size": "$2020"
                },
                {
                    "detect": true
                }
            ]
        }
    }
}