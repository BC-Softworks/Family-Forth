{
    "$schema": "../../../../schema/testcase.schema.json",
    "target": {
        "testType": "JSR",
        "start": {
            "label": "TWO_FETCH"
        }
    },
    "testCases": {
        "$0600 2@": {
            "setup": {
                "register": {
                    "X": {
                        "value": "$FE"
                    }
                },
                "memory": [
                    {
                        "address": "$FE",
                        "value": "$00"
                    },
                    {
                        "address": "$FF",
                        "value": "$06"
                    },
                    {
                        "address": "$0600",
                        "value": "$02"
                    },
                    {
                        "address": "$0601",
                        "value": "$03"
                    },
                    {
                        "address": "$0602",
                        "value": "$00"
                    },
                    {
                        "address": "$0603",
                        "value": "$01"
                    }

                ]
                
            },
            "expected": {
                "register": {
                    "X": {
                        "value": { 
                            "eq": "$FC"
                        }
                    }
                },
                "memory": [
                    {
                        "address": "$FC",
                        "value": {
                            "eq": "$00"
                        }
                    },
                    {
                        "address": "$FD",
                        "value": {
                            "eq": "$01"
                        }
                    },
                    {
                        "address": "$FE",
                        "value": {
                            "eq": "$02"
                        }
                    },
                    {
                        "address": "$FF",
                        "value": {
                            "eq": "$03"
                        }
                    }
                ]
            }
        }
    },
    "config": {
        "testMemoryAccess": {
            "feature": [
                "ACCESS_TO_OUT_OF_SEGMENT"
            ],
            "ignoreList": [
                {
                    "start": {
                        "address": "$0"
                    },
                    "size": "$800"
                },
                {
                    "start": {
                        "address": "$2000"
                    },
                    "size": "$2020"
                },
                {
                    "detect": true
                }
            ]
        }
    }
}
